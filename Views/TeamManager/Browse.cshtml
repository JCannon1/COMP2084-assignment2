@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model Assignment2.Models.HockeyModel

<h2>Teams for: @Model.Table_HockeyTeams</h2>

<table class="table table-striped table-hover">
    <tr>
        <th>Title</th>
        <th>Team</th>
        <th>Player</th>
    </tr>

    @foreach (var th in Model.Table_HockeyTeams)
    {
        <tr>
            <td> th.Title</td>
            <td> th.Name</td>
            <td> th.City</td>
        </tr>
    }
</table>

